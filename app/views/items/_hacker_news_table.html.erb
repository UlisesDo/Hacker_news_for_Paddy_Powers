<table>
	  <tr class="header">
	    <th>Title</th>
	    <th>URL</th>
	    <th>Hacker News Id</th>
	    <th>Number of comments</th>
	    <th>Points</th>
	    <th>Posted ago</th>
	    <th>Posted By</th>
	  </tr>
	 
	<% @items.each do |item| %>
	  <% if item["points"] > @median_value %>
	  	<tr class="interesting">
	  <% else %>
	  	<tr>
	  <% end %>		
	    <td><%= item["title"] %></td>
	    <td><%= item["url"] %></td>
	    <td><%= item["id"] %></td>
	    <td><%= item["commentCount"] %></td>
	    <td class="points"><%= item["points"] %></td>
	    <td><%= item["postedAgo"] %></td>
	    <td><%= item["postedBy"] %></td>
	  </tr>
	<% end %>
</table>